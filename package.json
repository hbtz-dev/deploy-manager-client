{
  "name": "deploy-manager-client",
  "version": "0.0.1",
  "description": "client for use with deploy-manager",
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "cross-env-shell echo VITE_MANAGER_LOCATION=$VITE_MANAGER_LOCATION > .env && tsc && npx vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "npx vite preview",
    "start": "cross-env-shell npx serve dist/ --no-port-switching -l $PORT"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hbtz-dev/deploy-manager-client.git"
  },
  "author": "hbtz-dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hbtz-dev/deploy-manager-client/issues"
  },
  "homepage": "https://github.com/hbtz-dev/deploy-manager-client#readme",
  "dependencies": {
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use-websocket": "^3.0.0",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}